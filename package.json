{
  "name": "lode3",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "forever:start": "cross-env NODE_ENV=production forever start -a -o out.log -e err.log dist/server/index.js",
    "forever:stop": "forever stopall",
    "build:prod": "cross-env NODE_ENV=production npm run client:build:prod && npm run server:build:prod",
    "client:serve": "ng serve --proxy-config proxy.conf.js",
    "client:serve:prod": "ng serve --proxy-config proxy.conf.js --prod",
    "client:build": "ng build",
    "client:build:w": "ng build -w",
    "client:build:prod": "cross-env NODE_ENV=production ng build --prod",
    "client:test": "ng test",
    "client:lint": "ng lint",
    "client:e2e": "ng e2e",
    "server:start:w": "nodemon dist/server/index.js",
    "server:build": "tsc -p src/server/tsconfig.app.json && npm run server:postbuild",
    "server:build:w": "tsc -p src/server/tsconfig.app.json -w",
    "server:build:prod": "cross-env NODE_ENV=production tsc -p src/server/tsconfig.app.json --sourceMap false && npm run server:postbuild",
    "server:postbuild": "cp -r src/server/resources dist/server/resources",
    "clean": "rm -r dist",
    "l3:build:w": "tsc -p src/l3client/tsconfig.app.json -w",
    "l3:start:w": "nodemon dist/l3client/index.js"
  },
  "private": true,
  "dependencies": {

{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "outDir": "../bin",
    "rootDir": "./",
    "noImplicitAny": false
  },
  "exclude": [
    "node_modules"
  ]
}
